{
  "name": "better_new_tab_client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production run-s lint tailwind:build type-check \"build-only --mode production --minify\" test:run",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:run": "vitest run",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "tailwind:watch": "tailwindcss -i ./src/assets/css/main.css -o ./src/assets/css/tailwind.css --watch",
    "tailwind:build": "tailwindcss -i ./src/assets/css/main.css -o ./src/assets/css/tailwind.css --minify",
    "dev:all": "run-p dev tailwind:watch",
    "lint": "biome check --write",
    "start": "bun lint && bun dev:all",
    "build:all": "NODE_ENV=production run-s tailwind:build \"build-only --mode production --minify\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.1",
    "@types/axios": "^0.14.4",
    "@types/lodash": "^4.17.15",
    "@unhead/vue": "^1.11.20",
    "axios": "^1.7.9",
    "fuse.js": "^7.0.0",
    "lodash": "^4.17.21",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.0.1",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-compression2": "^1.3.3",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^3.0.4",
    "vue-tsc": "^2.2.0"
  }
}
