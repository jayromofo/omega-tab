{
	"name": "omega-tab-landing",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "NODE_ENV=production run-s tailwind:build type-check \"build-only --mode production --minify\"",
		"preview": "vite preview",
		"build-only": "vite build",
		"type-check": "vue-tsc --build",
		"tailwind:watch": "tailwindcss -i ./src/assets/css/main.css -o ./src/assets/css/tailwind.css --watch",
		"tailwind:build": "tailwindcss -i ./src/assets/css/main.css -o ./src/assets/css/tailwind.css --minify",
		"dev:all": "run-p dev tailwind:watch",
		"start": "bun dev:all"
	},
	"dependencies": {
		"vue": "^3.5.13",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^5.2.1",
		"@vue/tsconfig": "^0.7.0",
		"npm-run-all2": "^7.0.2",
		"tailwindcss": "^3.4.17",
		"typescript": "~5.7.3",
		"vite": "^6.0.11",
		"vue-tsc": "^2.2.0"
	}
}
